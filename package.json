{
  "name": "rest-express",
  "version": "1.0.0",
  "description": "Template project for Node.js Express RESTful API",
  "scripts": {
    "build": "npx rimraf dist && npx tsc --project ./",
    "start": "npx nodemon src/server.ts",
    "lint": "npx eslint . --ext .ts --fix",
    "format": "npx prettier --config .prettierrc.json . --write",
    "prepare": "husky install"
  },
  "keywords": [],
  "author": "Alex Tran",
  "license": "MIT",
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  },
  "dependencies": {
    "cors": "2.8.5",
    "date-fns": "2.30.0",
    "dotenv": "16.3.1",
    "express": "4.18.2",
    "http-status-codes": "2.2.0",
    "husky": "8.0.3",
    "lodash": "4.17.21",
    "mongodb-memory-server": "8.13.0",
    "mongoose": "7.3.1",
    "uuid": "9.0.0",
    "winston": "3.9.0"
  },
  "devDependencies": {
    "@faker-js/faker": "8.0.2",
    "@types/cors": "2.8.13",
    "@types/express": "4.17.17",
    "@types/jest": "29.5.2",
    "@types/lodash": "4.14.195",
    "@types/node": "^18",
    "@types/uuid": "9.0.2",
    "@typescript-eslint/eslint-plugin": "^5.60.0",
    "@typescript-eslint/parser": "^5.60.0",
    "eslint": "8.43.0",
    "eslint-plugin-prettier": "4.2.1",
    "jest": "29.5.0",
    "nodemon": "2.0.22",
    "prettier": "2.8.8",
    "rimraf": "5.0.1",
    "ts-node": "10.9.1",
    "typescript": "5.0.3"
  }
}
